extend layout
block content
  div(class="row")
    div(class="col-md-6 col-md-offset-3 text-center")
      table(class="table")
        - for(var key in clips)
          tr
            td=clips[key].title
            td
              a(href="#" data-lefilename=clips[key].path) Play
              a(href="#") Stop

  script.
    $("a").on("click", function(e) {
      MIDIjs.stop();
      if(e.target.dataset.lefilename)
        MIDIjs.play(e.target.dataset.lefilename);
    });
